<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommand>mkb.exe</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(SolutionDir)\examples</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerDebuggerType>ManagedCore</LocalDebuggerDebuggerType>
    <LocalDebuggerCommandArguments>-ksrb -ksdbg test</LocalDebuggerCommandArguments>
    <LocalDebuggerCommandArgumentsHistory>help|-ksrb -ksdbg help|-ksrb -ksdbg test|</LocalDebuggerCommandArgumentsHistory>
  </PropertyGroup>
  <PropertyGroup>
    <ShowAllFiles>true</ShowAllFiles>
  </PropertyGroup>
</Project>